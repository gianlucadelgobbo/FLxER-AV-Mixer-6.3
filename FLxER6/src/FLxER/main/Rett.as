package FLxER.main{    import flash.display.Sprite;	public class Rett extends Sprite {		public var enabled:Boolean;		public function Rett(xx:Number,yy:Number,w:uint,h:uint,col:Number,o_col:Number,aa:Number):void {			x=xx;			y=yy;			if (o_col != -1) {				graphics.lineStyle(0,o_col,aa);			}			if (col != -1) {				graphics.beginFill(col,aa);			}			graphics.moveTo(0,0);			graphics.lineTo(w,0);			graphics.lineTo(w,h);			graphics.lineTo(0,h);			graphics.lineTo(0,0);		}	}}