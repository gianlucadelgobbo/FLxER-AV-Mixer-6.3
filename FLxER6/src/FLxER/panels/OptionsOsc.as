package FLxER.panels {	import FLxER.comp.ButtonTxt;	import FLxER.comp.CheckBoxBase;	import FLxER.comp.ListMenu;	import FLxER.main.Txt;	import FLxER.panels.Palette;//	import FLxER.core.OSC;	public class OptionsOsc extends Palette {		private var cntObj:Object		private var callback:Function		private var onclose:Function		private var map:String;		private var btn:CheckBoxBase;		private var c:int		public function OptionsOsc(w:int,h:int,t:String,b:CheckBoxBase,fnz:Function,fnzclose:Function):void {			super(w,h,t+" SETTINGS","");			callback = fnz;			onclose = fnzclose;			btn = b;			map = Preferences.pref.osc.map ? Preferences.pref.osc.map.name : "select map"			cntObj = new Object();			cntObj.osc0 		= new Txt(5, 30, 200, 15, t+" SERVER ADDRESS ", Preferences.ts, null);			cntObj.oscF 		= new Txt(140, 30, 80, 15, Preferences.pref.osc.server, Preferences.th, "input");			cntObj.osc1 		= new Txt(100, 60, 50, 15, "PORT ", Preferences.ts, null);			cntObj.oscPortF 	= new Txt(140, 60, 40, 15, Preferences.pref.osc.port, Preferences.th, "input");			cntObj.mapL 		= new Txt(100, 90, 50, 15, "MAP ", Preferences.ts, null);			cntObj.mapF	 	= new ListMenu(140, 90, 150, 15, map, setMap, null, Preferences.oscMaps.childNodes[0], 3);			cntObj.activeL		= new Txt(75, 120, 50, 15, "STATUS ", Preferences.ts, null);			cntObj.activeF	 	= new CheckBoxBase(140, 120, 15, 15, t, loadLib2, null, Preferences.OSCactive);			cntObj.pulsSave 	= new ButtonTxt(100, 150, 50, 15, "SAVE", mySave, null, null);			if (!Preferences.pref.osc.map) cntObj.pulsSave.myDisable();						cntObj.pulsCancel 	= new ButtonTxt(160, 150, 50, 15, "CANCEL", myCancel, null, null);			for (var item:* in cntObj) {				this.palette.stage.addChild(cntObj[item])			}			//mode0.scaleX = mode0.scaleY = 			//mode1.scaleX = mode1.scaleY = 			//mode2.scaleX = mode2.scaleY = 2			//c = setInterval(bella,2000);		}		private function bella():void {			trace("OptionsOsc.");		}		public function setMap(a,b):void {			map = a;			Preferences.pref.osc.map = {name:a, url:b};			cntObj.pulsSave.myEnable();		}		public function loadLib2(a):void {		}		public function mySave(a):void {			Preferences.pref.osc.server = cntObj.oscF.text			Preferences.pref.osc.port = cntObj.oscPortF.text			Preferences.starterTrgt.oscOnOff(cntObj.activeF.myStatus);			btn.myStatus = !cntObj.activeF.myStatus;			btn.myStatus_swap();			callback();			this.palette.close();			onclose();		}		public function myCancel(a):void {			btn.myStatus = !cntObj.activeF.myStatus;			btn.myStatus_swap();			this.palette.close();			onclose();		}	}}