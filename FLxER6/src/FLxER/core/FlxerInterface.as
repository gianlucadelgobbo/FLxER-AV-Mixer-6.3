package FLxER.core {	import flash.display.Sprite;		import FLxER.panels.ChCtrl;	public class FlxerInterface extends Sprite {		public var chCnt:Object;		public function FlxerInterface():void {			Preferences.interfaceTrgt = this;			chCnt = new Object();			for (var a:int = 0; a<Preferences.pref.nCh; a++) {				chCnt["ch_"+a] = new ChCtrl(a);				this.addChild(chCnt["ch_"+a]);			}			chCnt.ch_0.change_ch();		}	}}